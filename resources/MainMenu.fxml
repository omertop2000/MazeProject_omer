<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>

<Scene stylesheets="@MainMenuCSS.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.MainMenuController">
    <AnchorPane fx:id="mainPane" prefHeight="598.0" prefWidth="786.0">
        <children>
            <ImageView fx:id="mainImageView" pickOnBounds="true" preserveRatio="false" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                <image>
                    <Image url="/images/BackgroundGif.gif"/>
                </image>
            </ImageView>
            <StackPane minWidth="0" minHeight="0" layoutX="180.0" layoutY="169.0" prefHeight="215.0" prefWidth="373.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                <children>
                    <Button fx:id="Start_button" mnemonicParsing="false" onAction="#buttonStart" prefHeight="26.0" prefWidth="323.0" text="New Game" >
                        <StackPane.margin>
                            <Insets bottom="150.0" top="-20.0" />
                        </StackPane.margin>
                    </Button>
                    <Button fx:id="button_Load" mnemonicParsing="false" onAction="#buttonLoad" prefHeight="72.0" prefWidth="323.0" text="Load">
                        <StackPane.margin>
                            <Insets top="-10.0" />
                        </StackPane.margin>
                    </Button>
                    <Button fx:id="button_Options" mnemonicParsing="false" onAction="#buttonOptions" prefHeight="31.0" prefWidth="320.0" text="Options">
                        <StackPane.margin>
                            <Insets top="150.0" />
                        </StackPane.margin>
                    </Button>
                    <Button fx:id="Button_About" mnemonicParsing="false" onAction="#buttonAbout" prefHeight="31.0" prefWidth="320.0" text="About">
                        <StackPane.margin>
                            <Insets top="310.0" />
                        </StackPane.margin>
                    </Button>
                    <Button fx:id="Button_Exit" mnemonicParsing="false" onAction="#buttonExit" prefHeight="31.0" prefWidth="320.0" text="Exit">
                        <StackPane.margin>
                            <Insets top="470.0" />
                        </StackPane.margin>
                    </Button>
                </children>
            </StackPane>
        </children>
    </AnchorPane>
</Scene>